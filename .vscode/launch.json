{
  // 使用 IntelliSense 了解相关属性。 
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "使用本机 Chrome 调试",
      "type": "chrome",
      "request": "launch",
      "file": "${workspaceRoot}/src/main/resources/templates/about.html",
      //  "url": "http://mysite.com/index.html", //使用外部服务器时,请注释掉 file, 改用 url, 并将 useBuildInServer 设置为 false "http://mysite.com/index.html
      "runtimeExecutable": "C:\\Users\\Administrator\\AppData\\Local\\Google\\Chrome\\Application\\chrome.exe", // 改成您的 Chrome 安装路径
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      //  "preLaunchTask":"build",
      "userDataDir": "${tmpdir}",
      "port": 5433
    },
    {
      "type": "java",
      "name": "Launch ProjectApplication",
      "request": "launch",
      "mainClass": "com.bjev.esb.ProjectApplication",
      "projectName": "springboot-manager"
    },
    {
      "type": "java",
      "name": "Launch BjevProjectApplication",
      "request": "launch",
      "mainClass": "com.bjev.esb.BjevProjectApplication",
      "projectName": "springboot-manager"
    },
    {
      "type": "java",
      "name": "Launch Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Launch bjevProjectApplication",
      "request": "launch",
      "mainClass": "com.bjev.esb.bjevProjectApplication",
      "projectName": "springboot-manager"
    },
    {
      "type": "java",
      "name": "Launch HttpContextUtils",
      "request": "launch",
      "mainClass": "com.bjev.esb.common.utils.HttpContextUtils",
      "projectName": "springboot-manager"
    },
    {
      "type": "java",
      "name": "Launch PasswordEncoder",
      "request": "launch",
      "mainClass": "com.bjev.esb.common.utils.PasswordEncoder",
      "projectName": "springboot-manager"
    }
  ]
}